var bad = 0;

forHash trueTests do name f:
    try
        if not (f ()) then
            bad := bad + 1;
            println "\(name) failed."
        fi
    catch java.lang.Throwable ex:
        bad := bad + 1;
        println "\(name) failed: \(stackTraceString ex)"
    yrt
done;

total = length trueTests;
println "Testing done, \(total - bad)/\(total) OK.";
